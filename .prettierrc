{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "endOfLine": "lf",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "overrides": [
    {
      "files": [".eslintrc", ".eslintrc.json"],
      "options": {
        "trailingComma": "none",
        "parser": "json"
      }
    }
  ]
}
